{
  "name": "@birdseye/core",
  "version": "0.4.0",
  "author": "katashin",
  "description": "Core modules of Birdseye",
  "keywords": [
    "Birdseye",
    "core"
  ],
  "license": "MIT",
  "main": "dist/core.cjs.js",
  "module": "dist/core.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ktsn/birdseye",
  "bugs": "https://github.com/ktsn/birdseye/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/birdseye.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "yarn clean && yarn build",
    "prepublishOnly": "yarn test",
    "clean": "rm -rf dist",
    "play": "poi --open --config play.config.js",
    "build": "node build.js",
    "build:play": "poi build --config play.config.js",
    "lint": "eslint --ext js,ts,vue src test",
    "lint:fix": "yarn lint --fix",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/jest": "^25.1.0",
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-core": "7.0.0-bridge.0",
    "eslint": "^6.5.1",
    "eslint-config-ktsn-vue": "^2.0.0",
    "jest": "^25.1.0",
    "postcss": "^7.0.2",
    "prettier": "2.0.5",
    "prettier-config-ktsn": "^1.0.0",
    "rollup": "^2.0.0",
    "rollup-plugin-css-only": "^2.0.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "rollup-plugin-vue": "^5.0.1",
    "ts-jest": "^25.0.0",
    "typescript": "^3.0.3",
    "vue": "^2.6.7",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.7"
  }
}
