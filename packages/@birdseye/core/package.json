{
  "name": "@birdseye/core",
  "version": "0.1.0",
  "author": "katashin",
  "description": "Core modules of Birdseye",
  "keywords": [
    "Birdseye",
    "core"
  ],
  "license": "MIT",
  "main": "dist/core.es.js",
  "module": "dist/core.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ktsn/birdseye",
  "bugs": "https://github.com/ktsn/birdseye/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/birdseye.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run release",
    "clean": "rm -rf dist",
    "play": "poi --open --config play.config.js",
    "build": "rollup -c",
    "build:play": "poi build --config play.config.js",
    "lint": "eslint --ext js,ts,vue src test",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint",
    "test:unit": "jest",
    "test:dev": "jest --watch",
    "release": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@types/jest": "^23.3.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "eslint": "^5.1.0",
    "eslint-config-ktsn-vue": "^1.0.1",
    "jest": "^23.4.1",
    "poi": "^10.2.9",
    "postcss": "^7.0.2",
    "prettier": "1.14.0",
    "prettier-config-ktsn": "^1.0.0",
    "rollup": "^0.63.4",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-typescript2": "^0.16.1",
    "rollup-plugin-vue": "^4.3.1",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "vue": "^2.5.16",
    "vue-jest": "^2.6.0",
    "vue-play": "^3.2.1",
    "vue-template-compiler": "^2.5.16"
  }
}