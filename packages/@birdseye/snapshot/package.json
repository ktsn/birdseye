{
  "name": "@birdseye/snapshot",
  "version": "0.9.2",
  "author": "katashin",
  "description": "Taking snapshots for Birdseye catalog",
  "keywords": [
    "Birdseye",
    "snapshot",
    "testing",
    "visual regression"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/ktsn/birdseye",
  "bugs": "https://github.com/ktsn/birdseye/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/birdseye.git"
  },
  "scripts": {
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "clean": "rm -rf lib",
    "serve": "vue-cli-service serve test/fixture/main.ts",
    "build": "tsc -p src",
    "dev": "jest --watch",
    "lint": "eslint --ext js,ts src test",
    "lint:fix": "eslint --fix --ext js,ts src test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/test/.+\\.spec\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest-setup.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    }
  },
  "devDependencies": {
    "@birdseye/app": "^0.9.2",
    "@types/jest": "^26.0.0",
    "@types/jest-image-snapshot": "^4.1.0",
    "@types/mkdirp": "^1.0.0",
    "@types/puppeteer": "^5.4.0",
    "@types/rimraf": "^3.0.0",
    "@vue/cli-plugin-typescript": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "eslint": "^7.0.0",
    "eslint-config-ktsn-typescript": "^2.0.0",
    "jest": "^26.0.1",
    "jest-image-snapshot": "^4.0.0",
    "prettier": "2.2.1",
    "prettier-config-ktsn": "^1.0.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^26.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@birdseye/core": "^0.9.0",
    "@birdseye/vue": "^0.9.2",
    "capture-all": "^0.7.1",
    "mkdirp": "^1.0.3",
    "puppeteer": "^5.2.0"
  }
}
