{
  "name": "@birdseye/vue",
  "version": "0.1.0",
  "author": "katashin",
  "description": "Vue.js integration of Birdseye",
  "keywords": [
    "Birdseye",
    "Vue.js",
    "component",
    "styleguide"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "webpack-loader.js"
  ],
  "homepage": "https://github.com/ktsn/birdseye",
  "bugs": "https://github.com/ktsn/birdseye/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/birdseye.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "yarn clean && yarn build",
    "prepublishOnly": "yarn test",
    "clean": "rm -rf lib",
    "build": "tsc -p src",
    "dev": "jest --watch",
    "lint": "eslint --ext js,ts src test",
    "lint:fix": "eslint --fix --ext js,ts src test",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "/test/.+\\.spec\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "@types/js-yaml": "^3.11.2",
    "@types/loader-utils": "^1.1.3",
    "@types/node": "^10.5.4",
    "@types/webpack": "^4.4.8",
    "@vue/test-utils": "^1.0.0-beta.23",
    "eslint": "^5.2.0",
    "eslint-config-ktsn-typescript": "^1.1.3",
    "jest": "^23.4.1",
    "prettier": "1.13.7",
    "prettier-config-ktsn": "^1.0.0",
    "ts-jest": "^23.0.1",
    "typescript": "^2.9.2",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.3"
  },
  "dependencies": {
    "@birdseye/core": "^0.1.0",
    "js-yaml": "^3.12.0",
    "loader-utils": "^1.1.0"
  }
}